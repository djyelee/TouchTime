<?xml version="1.0" encoding="utf-8"?>
<com.svw.touchtime.CheckableLinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="horizontal" >

    <TextView
        android:id="@+id/textViewID"
        android:focusable="false"
        android:layout_height="wrap_content"
        android:layout_width="0dp"
        android:layout_weight="1"
        android:paddingLeft="5dp"
        android:layout_marginEnd="10dp"
        android:layout_marginRight="10dp"
        android:textColor="#000000"
        android:textSize="@dimen/medium_small_text_size"
        android:layout_centerVertical="true"
        />
    <TextView
        android:id="@+id/textViewLastName"
        android:layout_height="wrap_content"
        android:layout_width="0dp"
        android:layout_weight="3"
        android:paddingLeft="5dp"
        android:layout_marginEnd="10dp"
        android:layout_marginRight="10dp"
        android:textColor="#000000"
        android:textSize="@dimen/medium_small_text_size"
        android:layout_centerVertical="true"
        android:focusable="false"
        android:singleLine="true"
        />

    <TextView
        android:id="@+id/textViewFirstName"
        android:layout_height="wrap_content"
        android:layout_width="0dp"
        android:layout_weight="3"
        android:paddingLeft="5dp"
        android:layout_marginEnd="10dp"
        android:layout_marginRight="10dp"
        android:textColor="#000000"
        android:textSize="@dimen/medium_small_text_size"
        android:layout_centerVertical="true"
        android:focusable="false"
        android:singleLine="true"
        />

    <TextView
        android:id="@+id/textViewGroup"
        android:layout_height="wrap_content"
        android:layout_width="0dp"
        android:layout_weight="1.2"
        android:paddingLeft="5dp"
        android:layout_marginEnd="10dp"
        android:layout_marginRight="10dp"
        android:textColor="#000000"
        android:textSize="@dimen/medium_small_text_size"
        android:layout_centerVertical="true"
        android:focusable="false"
        />

    <TextView
        android:id="@+id/textViewCompany"
        android:layout_height="wrap_content"
        android:layout_width="0dp"
        android:layout_weight="2"
        android:paddingLeft="5dp"
        android:layout_marginEnd="10dp"
        android:layout_marginRight="10dp"
        android:textColor="#000000"
        android:textSize="@dimen/medium_small_text_size"
        android:layout_centerVertical="true"
        android:focusable="false"
        />

    <TextView
        android:id="@+id/textViewLocation"
        android:layout_height="wrap_content"
        android:layout_width="0dp"
        android:layout_weight="2"
        android:paddingLeft="5dp"
        android:layout_marginEnd="10dp"
        android:layout_marginRight="10dp"
        android:textColor="#000000"
        android:textSize="@dimen/medium_small_text_size"
        android:layout_centerVertical="true"
        android:focusable="false"
        />

    <TextView
        android:id="@+id/textViewJob"
        android:layout_height="wrap_content"
        android:layout_width="0dp"
        android:layout_weight="2"
        android:paddingLeft="5dp"
        android:layout_marginEnd="10dp"
        android:layout_marginRight="10dp"
        android:textColor="#000000"
        android:textSize="@dimen/medium_small_text_size"
        android:layout_centerVertical="true"
        android:focusable="false"
        />

    <TextView
        android:id="@+id/textViewDate"
        android:layout_height="wrap_content"
        android:layout_width="0dp"
        android:layout_weight="1.2"
        android:paddingLeft="5dp"
        android:layout_marginEnd="10dp"
        android:layout_marginRight="10dp"
        android:textColor="#000000"
        android:textSize="@dimen/medium_small_text_size"
        android:layout_centerVertical="true"
        android:focusable="false"
        android:gravity="center"
        />

    <TextView
        android:id="@+id/textViewTimeIn"
        android:layout_height="wrap_content"
        android:layout_width="0dp"
        android:layout_weight="1.4"
        android:paddingLeft="5dp"
        android:layout_marginEnd="10dp"
        android:layout_marginRight="10dp"
        android:textColor="#000000"
        android:textSize="@dimen/medium_small_text_size"
        android:layout_centerVertical="true"
        android:focusable="false"
        android:gravity="center"
        />

    <TextView
        android:id="@+id/textViewTimeOut"
        android:layout_height="wrap_content"
        android:layout_width="0dp"
        android:layout_weight="1.4"
        android:paddingLeft="5dp"
        android:layout_marginEnd="10dp"
        android:layout_marginRight="10dp"
        android:textColor="#000000"
        android:textSize="@dimen/medium_small_text_size"
        android:layout_centerVertical="true"
        android:focusable="false"
        android:gravity="center"
        />
    <TextView
        android:id="@+id/textViewLunch"
        android:layout_height="wrap_content"
        android:layout_width="0dp"
        android:layout_weight="1"
        android:paddingLeft="5dp"
        android:layout_marginEnd="10dp"
        android:layout_marginRight="10dp"
        android:textColor="#000000"
        android:textSize="@dimen/medium_small_text_size"
        android:layout_centerVertical="true"
        android:focusable="false"
        android:gravity="center"
        />

    <TextView
        android:id="@+id/textViewHours"
        android:layout_height="wrap_content"
        android:layout_width="0dp"
        android:layout_weight="1"
        android:paddingLeft="5dp"
        android:layout_marginEnd="10dp"
        android:layout_marginRight="10dp"
        android:textColor="#000000"
        android:textSize="@dimen/medium_small_text_size"
        android:layout_centerVertical="true"
        android:focusable="false"
        android:gravity="center"
        />

    <TextView
        android:id="@+id/textViewSupervisor"
        android:layout_height="wrap_content"
        android:layout_width="0dp"
        android:layout_weight="2"
        android:paddingLeft="5dp"
        android:layout_marginEnd="10dp"
        android:layout_marginRight="10dp"
        android:textColor="#000000"
        android:textSize="@dimen/medium_small_text_size"
        android:layout_centerVertical="true"
        android:focusable="false"
        />
    <TextView
        android:id="@+id/textViewComments"
        android:layout_height="wrap_content"
        android:layout_width="0dp"
        android:layout_weight="2"
        android:layout_marginEnd="10dp"
        android:layout_marginRight="10dp"
        android:textColor="#000000"
        android:textSize="@dimen/medium_small_text_size"
        android:layout_centerVertical="true"
        android:focusable="false"
        />

    <!--
   <EditText
        android:id="@+id/textViewComments"
        android:layout_height="wrap_content"
        android:layout_width="0dp"
        android:layout_weight="2"
        android:paddingLeft="5dp"
        android:layout_marginEnd="10dp"
        android:layout_marginRight="10dp"
        android:textColor="#000000"
        android:textSize="@dimen/small_text_size"
        android:layout_centerVertical="true"
        android:focusable="false"
        android:inputType="text"
        android:ems="10"/>
-->
</com.svw.touchtime.CheckableLinearLayout>

